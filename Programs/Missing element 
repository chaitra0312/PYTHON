
class Solution:
    def missingNumber(self, arr)
       n=len(arr)+1
       total_sum=n*(n+1)//2
       sum_arr=sum(arr)
       return total_sum-sum_arr

t = int(input())
for _ in range(0, t):
    arr = list(map(int, input().split()))
    s = Solution().missingNumber(arr)
    print(s)
    print("~")
